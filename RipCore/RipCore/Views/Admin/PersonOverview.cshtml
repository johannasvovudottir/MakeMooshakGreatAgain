@model List<RipCore.Models.ViewModels.PersonViewModel>
@{
    ViewBag.Title = "PersonOverview";
}

<h2>PersonOverview</h2>

<p>All persons should be displayed here</p>

<ul>
    @foreach (var item in Model)
    {
        <li>
            @Html.ActionLink(item.Name, "EditPerson", new { id = item.ID })
        </li>
    }
</ul>